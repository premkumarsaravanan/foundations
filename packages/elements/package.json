{
  "name": "@reapit/elements",
  "version": "0.5.34",
  "description": "A collection of React components and utilities for building apps for Reapit Marketplace",
  "main": "dist/index.js",
  "umd:main": "dist/elements.umd.production.js",
  "module": "dist/elements.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "cross-env TZ=UTC jest --coverage --silent",
    "test-dev": "cross-env TZ=UTC jest --watch --verbose",
    "test:ci": "cross-env TZ=UTC jest  --coverage --silent --detectOpenHandles --forceExit",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production tsdx build --format=cjs,esm,umd && rollup -c",
    "start": "cross-env NODE_ENV=development tsdx watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o out",
    "release:develop": "node scripts/releaseDevelop.js",
    "release:master": "node scripts/releaseMaster.js",
    "checkVersion": "node ./scripts/checkVersion.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reapit/elements.git"
  },
  "author": "Will McVay <wmcvay@reapit.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reapit/elements/issues"
  },
  "homepage": "https://github.com/reapit/elements#readme",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@storybook/addon-actions": "^5.2.6",
    "@storybook/addon-docs": "^5.2.6",
    "@storybook/addon-info": "^5.2.6",
    "@storybook/addon-links": "^5.2.6",
    "@storybook/addon-storysource": "^5.2.6",
    "@storybook/addon-viewport": "^5.2.6",
    "@storybook/addons": "^5.2.6",
    "@storybook/cli": "^5.2.6",
    "@storybook/react": "^5.2.6",
    "@storybook/storybook-deployer": "^2.8.1",
    "@storybook/theming": "^5.2.8",
    "@types/storybook__react": "^4.0.2",
    "@types/papaparse": "^5.0.3",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "compare-versions": "^3.5.1",
    "css-loader": "^3.2.0",
    "mockdate": "^2.0.5",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "purgecss": "^1.3.0",
    "purify-css": "^1.2.5",
    "react-docgen-typescript-loader": "^3.3.0",
    "rimraf": "^2.7.0",
    "rollup": "1.27.13",
    "rollup-plugin-scss": "^1.0.2",
    "sass-loader": "^7.2.0",
    "style-loader": "^1.0.0",
    "ts-node": "^8.3.0",
    "tsdx": "^0.7.2",
    "tslib": "^1.10.0"
  },
  "dependencies": {
    "bulma": "^0.7.5",
    "formik": "^2.0.4",
    "hardtack": "^4.1.0",
    "himalaya": "^1.1.0",
    "jsonwebtoken": "^8.5.1",
    "papaparse": "^5.1.1",
    "pell": "^1.0.6",
    "prop-types": "^15.7.2",
    "react-datasheet": "^1.4.0",
    "react-datepicker": "^2.9.6",
    "react-google-map": "^3.1.1",
    "react-google-maps-loader": "^4.2.5",
    "react-icons": "^3.7.0",
    "react-table": "7.0.0-alpha.35",
    "styled-components": "^4.4.1"
  },
  "peerDependencies": {
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 11"
  ]
}
