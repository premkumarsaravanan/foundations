{
  "name": "geo-diary",
  "version": "0.0.1",
  "private": true,
  "description": "Geo Diary App",
  "repository": {
    "type": "git",
    "url": "git+git@github.com:reapit/geo-diary.git"
  },
  "license": "MIT",
  "author": "Author",
  "main": "./src/index.ts",
  "scripts": {
    "build": "yarn fetch-config && yarn fetch-definitions && webpack --color --mode production --config ./src/scripts/webpack-prod.js",
    "dev": "yarn fetch-config && REAPIT_ENV=LOCAL webpack-dev-server --hot --progress --color --mode development --config ./src/scripts/webpack-dev.js",
    "fetch-config": "yarn config-manager getSecret reapit-marketplace-app-config",
    "fetch-definitions": "node ./src/scripts/fetch-definitions.js",
    "lint": "tslint --fix --project tsconfig.json --format stylish",
    "start": "node ./public/index.js geo-diary",
    "serve": "serve public/dist -s -l 8080",
    "test": "jest --coverage --silent",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit",
    "test-dev": "jest --watch --verbose",
    "test-dev-e2e": "wdio ./src/scripts/wdio.conf.js --watch",
    "test-e2e": "wdio ./src/scripts/wdio.conf.js",
    "test-e2e:bstack": "wdio ./src/scripts/browserstack.conf.js",
    "test-e2e:bstack:ci": "concurrently --success=\"first\" \"yarn start\" \"yarn test-e2e:bstack\" -k",
    "test-e2e:ci": "concurrently --success=\"first\" \"yarn start\" \"yarn test-e2e\" -k"
  },
  "dependencies": {
    "@reapit/cognito-auth": "^2.0.5",
    "@reapit/elements": "^0.5.2-beta",
    "dayjs": "^1.8.17",
    "hardtack": "^4.1.0",
    "localforage": "^1.7.3",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-icons": "^3.7.0",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-persist": "^5.10.0",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@reapit/config-manager": "^1.1.0",
    "@redux-saga/testing-utils": "^1.0.5",
    "@types/enzyme": "^3.9.3",
    "@types/googlemaps": "^3.37.5",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.2",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.0.9",
    "@types/react-router": "^5.1.2",
    "@types/react-router-dom": "^5.1.2",
    "@types/webdriverio": "^4.13.3",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "browserstack-capabilities": "^0.7.0",
    "chromedriver": "^2.43.1",
    "concurrently": "^4.1.1",
    "core-js": "^3.4.1",
    "css-loader": "^3.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "favicons-webpack-plugin": "^0.0.9",
    "file-loader": "^3.0.1",
    "fork-ts-checker-notifier-webpack-plugin": "^1.0.0",
    "fork-ts-checker-webpack-plugin": "^1.3.4",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.3.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^24.9.0",
    "jest-fetch-mock": "^2.1.2",
    "jest-junit": "^8.0.0",
    "lint-staged": "^8.1.7",
    "loader-utils": "^1.2.3",
    "mini-css-extract-plugin": "^0.7.0",
    "mockdate": "^2.0.5",
    "node-sass": "^4.12.0",
    "prettier": "^1.19.1",
    "prettier-plugin-packagejson": "^2.0.1",
    "purgecss": "^1.3.0",
    "purgecss-whitelister": "^2.4.0",
    "sass-loader": "^7.1.0",
    "serve": "^11.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "sw2dts": "^2.6.2",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.1",
    "ts-node": "^8.3.0",
    "ts-paths-to-webpack-alias": "^0.3.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.7.2",
    "wdio-browserstack-reporter": "^0.1.1",
    "wdio-chromedriver-service": "^0.1.3",
    "wdio-jasmine-framework": "^0.3.8",
    "wdio-spec-reporter": "^0.1.5",
    "webdriverio": "^4.14.4",
    "webpack": "^4.32.2",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "jest-junit": {
    "outputDirectory": "./.jest-junit-reports"
  }
}
