    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production tsdx build --format=cjs,esm,umd && rollup -c",
    "start": "cross-env NODE_ENV=development tsdx watch",
    "lint": "tslint --fix --project tsconfig.json --format stylish",
    "storybook": "start-storybook -p 6006",
