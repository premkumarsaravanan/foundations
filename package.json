{
  "name": "@reapit/web-components",
  "version": "0.0.3",
  "files": [
    "dist-npm/**/*"
  ],
  "main": "dist-npm/index.js",
  "types": "dist-npm/index.d.ts",
  "typings": "dist-npm/index.d.ts",
  "homepage": "https://demo-site.reapit.com",
  "dependencies": {
    "little-loader": "^0.2.0",
    "lodash.merge": "^4.6.2",
    "react-icons": "^3.7.0",
    "styled-components": "^4.4.0"
  },
  "peerDependencies": {
    "react": "^16.10.2",
    "react-dom": "^16.10.2"
  },
  "scripts": {
    "build:cdn-analyze": "ANALYZE=true webpack --config ./scripts/webpack.cdn.js",
    "build:npm-analyze": "ANALYZE=true webpack --config ./scripts/webpack.npm.js",
    "deploy:cdn": "aws s3 sync ./dist-cdn s3://reapit-web-components --delete --acl public-read",
    "build:cdn": "rimraf dist-cdn && webpack --config ./scripts/webpack.cdn.js",
    "build:npm": "rimraf dist-npm && webpack --config ./scripts/webpack.npm.js && yarn build:ts",
    "build:ts": "tsc -p './tsconfig.build.json'",
    "test": "jest --coverage --silent",
    "test-dev": "jest --watch",
    "lint": "eslint './src/**/*.ts' './src/**/*.tsx'",
    "lint:fix": "eslint --fix './src/**/*.ts' './src/**/*.tsx'",
    "prettify": "prettier -c --write \"*/**\"",
    "storybook": "start-storybook -p 8080",
    "build-storybook": "build-storybook",
    "release:master": "node scripts/releaseMaster.js",
    "checkVersion": "node ./scripts/checkVersion.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@octokit/rest": "^16.35.0",
    "@reapit/cognito-auth": "^2.0.10",
    "@reapit/foundations-ts-definitions": "^0.0.4",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/preset-typescript": "^1.1.0",
    "@storybook/react": "^5.2.8",
    "@testing-library/react-hooks": "^2.0.3",
    "@types/enzyme": "^3.10.3",
    "@types/googlemaps": "^3.38.0",
    "@types/jest": "^24.0.19",
    "@types/little-loader": "^0.2.0",
    "@types/lodash.merge": "^4.6.6",
    "@types/node": "12.7.12",
    "@types/react": "16.9.5",
    "@types/react-dom": "16.9.1",
    "@types/styled-components": "^4.1.19",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "compare-versions": "^3.5.1",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.7.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.0",
    "enzyme-to-json": "^3.4.2",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-react": "^1.1.7",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-emotion": "^10.0.14",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "file-loader": "^5.0.2",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "jest-fetch-mock": "^2.1.2",
    "lint-staged": "^9.4.2",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "prettier": "^1.18.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-test-renderer": "^16.12.0",
    "rimraf": "^3.0.0",
    "sw2dts": "^2.6.2",
    "ts-jest": "^24.1.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "3.6.3",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "alias": {
    "@/*": "./src/*"
  }
}
